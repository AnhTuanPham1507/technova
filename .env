###################################################
#              App Configuration                  #
#              config/app.ts                      #
###################################################
APP_PORT=3000
APP_ENV=development
APP_MODE=worker
APP_ENABLE_DOCUMENTATION=true
APP_TZ=UTC
APP_CORS_ORIGINS='["http://localhost:3000"]'
APP_FLUSH_CACHE_USER=cache
APP_FLUSH_CACHE_PASSWORD=hello123
APP_RATE_LIMIT_TTL=60000
APP_RATE_LIMIT_MAX=100


###################################################
#              API Authentication                 #
#              config/auth.ts                     #
###################################################
JWT_PRIVATE_KEY=test
JWT_EXPIRATION_TIME=15d
JWT_REFRESH_KEY=15d

###################################################
#              DB Configuration                   #
#              config/database.ts                 #
###################################################
DB_TYPE=postgres
DB_HOST=127.0.0.1
DB_PORT=5432
DB_APP_USER=root
DB_APP_PASS=root
DB_MIGRATION_USER=root
DB_MIGRATION_PASS=root
DB_NAME=technova_db
DB_LOGGING=["error"]
DB_CA_PATH=/path/to/certificate.crt

###################################################
#              Redis Configuration                #
#              config/redis.ts                    #
###################################################
REDIS_CACHE_ENABLED=true
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=password
REDIS_TTL=86400





