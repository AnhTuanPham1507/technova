    "express-ctx": "^0.1.1",
    "express-rate-limit": "^6.5.1",
    "express-session": "^1.17.3",
    "form-data": "^4.0.0",
    "helmet": "^6.0.0",
    "ioredis": "^5.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "module-alias": "^2.2.2",
    "moment": "2.24.0",
    "moment-timezone": "^0.5.43",
    "morgan": "^1.10.0",
    "nanoid": "3.x",
    "nest-winston": "^1.7.0",
    "nestjs-config": "^1.4.10",
    "openid-client": "^5.1.9",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
