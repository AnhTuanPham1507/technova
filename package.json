    "source-map-support": "^0.5.21",
    "streamifier": "^0.1.1",
    "swagger-ui-express": "^4.5.0",
    "typeorm": "^0.3.15",
    "typeorm-transactional": "^0.4.1",
    "typeorm-transactional-cls-hooked": "^0.1.21",
    "uuid": "^9.0.0",
    "uuidv4": "^6.2.13",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
