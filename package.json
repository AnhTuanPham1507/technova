{
  "name": "otribe-backend",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:prod": "nest build",
    "start:dev": "VERSION=$npm_package_version  nest start --debug --watch",
    "start:prod": "VERSION=$npm_package_version node dist/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "yarn typeorm migration:run -- -d ./ormconfig.ts",
    "migration:generate": "yarn typeorm -- -d ./ormconfig.ts migration:generate ./src/migrations/migrations/$npm_config_name",
    "migration:create": "yarn typeorm -- migration:create ./src/migrations/migrations/$npm_config_name",
    "migration:revert": "yarn typeorm -- -d ./ormconfig.ts migration:revert",
    "watch:dev": "ts-node-dev src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "copy:lib": "cpx 'src/lib/**/{files,workspace}/**/*.*' dist/lib && cpx 'src/lib/**/{files,workspace}/**/.!(gitignore)' dist/lib",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "repl": "npm run start:dev -- --entryFile repl"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.1",
    "@nestjs/common": "^9.0.11",
    "@nestjs/core": "^9.0.11",
    "@nestjs/cqrs": "^9.0.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.0.11",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "^6.0.5",
    "@nestjs/terminus": "^9.1.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/babel__core": "^7.20.0",
    "@types/module-alias": "^2.0.1",
    "aws-sdk": "^2.1196.0",
    "axios": "1.3.1",
    "base64url": "^3.0.1",
    "bcrypt": "^5.0.1",
    "bull": "^4.10.1",
